				<xs:element name="calculateRoutePattern" type="xs:boolean" maxOccurs="1" minOccurs="0"/>
				
				<xs:element name="sipTrunk" maxOccurs="1" minOccurs="0" >
					<xs:simpleType>
  						<xs:restriction base="xs:string">
							<xs:pattern value="aar|ars|[1-9]|[1-9][0-9]|[1-9]([0-9]){2}|[1]([0-9]){3}|2000|tg[1-9]|tg[1-9][0-9]|tg[1-9]([0-9]){2}|tg[1]([0-9]){3}|tg2000|rp[1-9]|rp[1-9][0-9]|rp[1-9]([0-9]){2}|rp[1]([0-9]){3}|rp2000"/>
