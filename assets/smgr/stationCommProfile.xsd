						<xs:restriction base="xs:string">
							<xs:pattern value="[1-9]{0}|[1-9]|[1-9][0-9]|[1-9]([0-9]){2}|[1]([0-9]){3}|2000"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="displayCallerId" type="xs:boolean" maxOccurs="1" minOccurs="0" />
				<xs:element name="callerIdMsgWaitingIndication" type="xs:boolean" maxOccurs="1" minOccurs="0" />
				<xs:element name="recallRotaryDigit" type="xs:boolean" maxOccurs="1" minOccurs="0" />
				<xs:element name="profileSettingsData" type="csm:xmlProfileSettings" maxOccurs="1" minOccurs="0" />
			</xs:sequence>
		</xs:extension>
   </xs:complexContent>
</xs:complexType>


